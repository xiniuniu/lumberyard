{
    "configuration_settings": {
        "linux_rpath": ["$ORIGIN"]
    },
    "platform": {
        "linux_x64": {
            "libpath_debug": [
                "lib/linux-x86_64-clang-debug"
            ],
            "libpath_release": [
                "lib/linux-x86_64-clang-release"
            ],
            "lib": [
                "libssl.a",
                "libcrypto.a"
            ],
            "sharedlibpath_debug": [
                "bin/linux-x86_64-clang-debug"
            ],
            "sharedlibpath_release": [
                "bin/linux-x86_64-clang-release"
            ],
            "shared": [
                "libssl.so",
                "libcrypto.so"
            ],
            "copy_extra": [
                "bin/linux-x86_64-clang-release/libssl.so.1.1:.",
                "bin/linux-x86_64-clang-release/libcrypto.so.1.1:."
            ],
	    "copy_extra_debug": [
	        "bin/linux-x86_64-clang-debug/libssl.so.1.1:.",
                "bin/linux-x86_64-clang-debug/libcrypto.so.1.1:."
	    ],
	    "copy_extra_release": [
	        "bin/linux-x86_64-clang-release/libssl.so.1.1:.",
                "bin/linux-x86_64-clang-release/libcrypto.so.1.1:."
	    ]
        }
    }
}
